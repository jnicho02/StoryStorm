<%= form_tag(search_path, method: "get", class: 'form-inline navbar-form pull-right') do %>
  <%= text_field_tag(:phrase, params[:phrase], type: 'text', placeholder: t('menu.search_placeholder'), class: 'form-control') %>
  <%= submit_tag(t('menu.go'), name: nil, class: 'btn btn-success-outline') %>
<% end %>
