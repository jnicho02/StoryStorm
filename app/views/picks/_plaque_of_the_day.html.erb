<% plaque = pick.plaque %>
<div class="row">
  <div class="col-xs-4">
    <%= render :partial => 'plaques/tile', locals: { plaque: plaque } %>
  </div>
  <div class="col-xs-8">
    <h4><%= link_to plaque.title, plaque %></h4>
    <h5><%= pick.proposer %> says,"<%= pick.description %>"</h5>
  </div>
</div>
