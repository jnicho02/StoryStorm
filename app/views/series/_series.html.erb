<div class="card">
  <div class="card-block">
    <h4 class="card-title">
      <%= link_to series.name, series %>
    </h4>
    <p class="card-text">
      <%= series.description %>
    </p>
  <div class="card-block">
    <%= render partial: 'plaques/tile', collection: series.plaques.photographed.limit(10), as: :plaque %>
  </div>
  </div>
</div>
